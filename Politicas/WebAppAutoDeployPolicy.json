{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "codebuild:*",
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "codepipeline:*",
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "codestar-connections:*",
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:CreateRole",
                "iam:AttachRolePolicy",
                "iam:PutRolePolicy",
                "iam:PassRole"
            ],
            "Resource": "arn:aws:iam::*:role/service-role/*"
        },
        {
            "Effect": "Allow",
            "Action": "iam:CreatePolicy",
            "Resource": "arn:aws:iam::*:policy/*"
        }
    ]
}